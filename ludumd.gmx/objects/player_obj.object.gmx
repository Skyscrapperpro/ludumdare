        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// Get rotation

if (!auto_rotation)
{
    //Horizontal movement
    global.pl_dangle = clamp_angle(global.pl_dangle - mouse_x_speed(), ANGREF);

    //Vertical movement
    if (ISPERSPECTIVE)
    if (global.handitem != 2) then zangle = clamp((zangle - mouse_y_speed()), -90, 90);
    else zangle = var_to_target(zangle, 0, AUTOCAM_SPD);
}
else //Auto rotation
{
    //Rotate
    global.pl_dangle = var_to_target(global.pl_dangle, 0, AUTOCAM_SPD);
    zangle = var_to_target(zangle, 0, AUTOCAM_SPD);
    
    //Check if rotation has finished
    if ((global.pl_dangle == 0) &amp;&amp; (zangle == 0))
        auto_rotation = false;
}

//Get real angle and real zangle
global.pl_rangle = clamp_angle(global.pl_dangle + 90, ANGREF);
global.pl_zangle = zreal(zangle);

</string>
