        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// Raycasts checking
if (global.shotframe || (LEFTCLICK &amp;&amp; (global.handitem != 2)))
{
    var obj_shot = nearest_raycasted(interactable_obj);
    if (obj_shot != noone)
    switch (global.handitem)
    {
        case 0:
//------------Spatula--------------//
if (LEFTCLICK) switch (obj_shot.object_index)
{
    case gunitem_obj:
        global.handitem = 2;
        instance_deactivate_object(gunitem_obj);
        break;
    case customer_obj:
        if (!instance_exists(zombie_obj) &amp;&amp; !obj_shot.satisf &amp;&amp; !obj_shot.dying)
            event_ask_customer(obj_shot);
        break;
    case trashcan_obj:
        spatulaburger = 0;
        break;
    case trash_plane:
        spatulaburger = 0;
        break;
    case burger_obj:
        if (spatulaburger == 0)
        {
            spatulaburger = obj_shot.cook + 1;
            with (obj_shot) instance_destroy();
        }
        break;
    case grill_obj:
        event_grill_burger(obj_shot, id);
        break;
    case burgerbox_obj:
        if (spatulaburger == 0) then spatulaburger = 1;
        if grillfogbool
            event_grillfog(grill_obj);
        break;
    case breadbox_obj:
        if (spatulaburger == 0) then spatulaburger = 4;
        break;
    case cheesebox_obj:
        if (spatulaburger == 0) then spatulaburger = 5;
        break;
    case lechugabox_obj:
        if (spatulaburger == 0) then spatulaburger = 6;
        break;
    case tomatobox_obj:
        if (spatulaburger == 0) then spatulaburger = 7;
        break;
    case plates_obj:
        if (spatulaburger != 0)
        {
            if event_place_ing(obj_shot, spatulaburger) then spatulaburger = 0;
        }
        else taken_plate = event_take_plate(obj_shot);
        break;
}
break;
        case 1:
//------------Plate--------------//
if (LEFTCLICK) switch (obj_shot.object_index)
{
    case gunitem_obj:
        event_putdown_plate(plates_obj, taken_plate);
        global.handitem = 2;
        instance_deactivate_object(gunitem_obj);
        break;
    case customer_obj:
        if (!instance_exists(zombie_obj) &amp;&amp; !obj_shot.satisf &amp;&amp; !obj_shot.dying)
        {
            if (event_compare_burger(obj_shot, taken_plate))
                event_satisfy_customer(obj_shot);
            else event_kill_customer(obj_shot);
                
            with (taken_plate)
            {
                ds_list_destroy(burger);
                instance_destroy();
            } global.handitem = 0;
        }
        break;
    case trashcan_obj:
        with (taken_plate) instance_destroy();
        global.handitem = 0;
        break;
    case trash_plane:
        with (taken_plate) instance_destroy();
        global.handitem = 0;
        break;
    case plates_obj:
        event_putdown_plate(obj_shot, taken_plate);
        global.handitem = 0;
        break;
}
break;
        case 2:
//------------Gun--------------//
if (global.shotframe)
{
    audio_play_sound(shot_snd, 2, false);
    switch (obj_shot.object_index)
    {
        case zombie_obj:
            event_kill_zombie(obj_shot);
            break;
        case customer_obj:
            event_kill_customer(obj_shot);
            break;
    }
}
break;
//-----------------------------//
    }
}

