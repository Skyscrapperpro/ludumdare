            <kind>1</kind>
            <string>///Get position
var correction;

if ((RIGHT or LEFT) &amp;&amp; (BACKWARDS or FORWARD)) //If pulsando diagonal
    correction=1-lengthdir_x(1, 45);
else
    correction=0; //Este ifelse crea la variable correction, que se le resta a 1 para que la diagonal no sea mayor que las paralelas a los ejes

if (BACKWARDS &amp;&amp; (xmodule &lt; (1 - correction))) { xmodule += 0.1; } //Velocidad progresiva según el tiempo que mantienes la tecla pulsada
else if (xmodule &gt; 0) { xmodule -= 0.1; }
if (FORWARD &amp;&amp; (xmodule &gt; (-1 + correction))) { xmodule -= 0.1; }
else if (xmodule &lt; 0) { xmodule += 0.1; }
if (LEFT &amp;&amp; (ymodule &lt; (1 - correction))) { ymodule += 0.1; }
else if (ymodule &gt; 0) { ymodule -= 0.1; }
if (RIGHT &amp;&amp; (ymodule &gt; (-1 + correction))) { ymodule -= 0.1; }
else if (ymodule &lt; 0) { ymodule += 0.1; }

//Angle
if ((abs(xmodule) &gt; 0.1)
or (abs(ymodule) &gt; 0.1))
    move_angle = angle + point_direction(0, 0, xmodule, ymodule);
else move_angle = angle;

//Speed
if (move_angle &gt; 360) then move_angle -= 360;
else if (move_angle &lt; 0) then move_angle += 360;

if (abs(xmodule) &gt; 0.1)
{
    if ((move_angle &gt; 90) &amp;&amp; (move_angle &lt; 270))
        xsonic = -lengthdir_x(xmodule, move_angle) * CHARSPD;
    else
        xsonic = lengthdir_x(xmodule, move_angle) * CHARSPD;
}
else
{
    xsonic = 0;
}
if (abs(ymodule) &gt; 0.1)
{
    if ((move_angle &gt; 0) &amp;&amp; (move_angle &lt; 180))
        ysonic = -lengthdir_y(ymodule, move_angle) * CHARSPD;
    else
        ysonic = lengthdir_y(ymodule, move_angle) * CHARSPD;
}
else
{
    ysonic = 0;
}

x += xsonic;
y += ysonic;

</string>
          </argument>
        </arguments>
