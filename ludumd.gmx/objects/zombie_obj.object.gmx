    if (subimg_animation_end(sprite, subi - s, s))
    {
        switch(sprite)
        {
            case ded_spr:
                if (instance_number(zombie_obj) == 1) then event_last_zombie();
                instance_destroy();
                break;
            case at_spr:
                sprite = walkf_spr;
                subi = 0;
                if (target != player_obj) //and
                if (point_distance_sqr(x, y, targetx, targety) &lt; ZOMBIE_ATTACK_RANGE_SQR)
