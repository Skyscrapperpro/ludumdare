            <string>///equip item and raycast (configure w, h and d)
w=sprite_get_width(burgersbox);
h=sprite_get_height(burgersbox);
d=40;

x1=x+lengthdir_x(w,player_obj.angle)*SIZESCALE/2;
y1=y+lengthdir_y(w,player_obj.angle)*SIZESCALE/2;
z1=0;
x2=x-lengthdir_x(w,player_obj.angle)*SIZESCALE/2;
y2=y-lengthdir_y(w,player_obj.angle)*SIZESCALE/2;
z2=d;

if ((player_obj.handitem=0) &amp;&amp; (player_obj.spatulaburger=0) &amp;&amp; RIGHTCLICK)
{
    angleto1=point_direction(player_obj.x,player_obj.y,x1,y1)-90;
    angleto2=point_direction(player_obj.x,player_obj.y,x2,y2)-90;
    zangleto1=point_direction(player_obj.x,player_obj.z,x1,z1);
    zangleto2=point_direction(player_obj.x,player_obj.z,x2,z2);
    anglemin=0;
    anglemax=0;
    target=point_direction(player_obj.x,player_obj.y,x,y)-90;
    
    while(angleto1&lt;0)
    {
        angleto1+=360;
    }
    while(angleto1&gt;=360)
    {
        angleto1-=360;
    }
    while(angleto2&lt;0)
    {
        angleto2+=360;
    }
    while(angleto2&gt;=360)
    {
        angleto2-=360;
    }
    while(target&lt;0)
    {
        target+=360;
    }
    while(target&gt;=360)
    {
        target-=360;
    }
    
    while(zangleto1&lt;-180)
    {
        zangleto1+=360;
    }
    while(zangleto1&gt;=180)
    {
        angleto1-=360;
    }
    while(zangleto2&lt;-180)
    {
        angleto2+=360;
    }
    while(angleto2&gt;=180)
    {
        angleto2-=360;
    }
    
    if (angleto1&lt;angleto2){anglemin=angleto1;anglemax=angleto2}
    else {anglemin=angleto2;anglemax=angleto1}
    
    
    if ((target&lt;anglemax) &amp;&amp; (target&gt;anglemin)){
        if ((player_obj.angle&gt;anglemin) &amp;&amp; (player_obj.angle&lt;anglemax) &amp;&amp; (player_obj.zangle&gt;zangleto1) &amp;&amp; (player_obj.zangle&lt;zangleto2) &amp;&amp; (point_distance(player_obj.x,player_obj.y,x,y)&lt;ITEMRANGE)){
            player_obj.spatulaburger=1;
        }
    }
    else
    {
        if (((player_obj.angle&lt;anglemin) || (player_obj.angle&gt;anglemax)) &amp;&amp; (player_obj.zangle&gt;zangleto1) &amp;&amp; (player_obj.zangle&lt;zangleto2) &amp;&amp; (point_distance(player_obj.x,player_obj.y,x,y)&lt;ITEMRANGE)){
            player_obj.spatulaburger=1;
        }
    }
}
</string>
